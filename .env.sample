# .env file
# --- Database Connection Details ---
# IMPORTANT: Set this to your MySQL HA endpoint (Load Balancer, Proxy, etc.)
DB_HOST=your_mysql_ha_endpoint_ip_or_dns
DB_PORT=3306
DB_USER=your_db_user
DB_PASSWORD=your_secret_password
DB_NAME=your_database_name
# Set to true to create database if it doesn't exist (optional)
CREATE_DB=false

# --- Test Client Configuration ---
WORKERS=10
SHORT_QUERY_INTERVAL=0.5
LONG_QUERY_CHANCE=0.1
LONG_QUERY_DURATION=10
CONNECT_TIMEOUT=10

